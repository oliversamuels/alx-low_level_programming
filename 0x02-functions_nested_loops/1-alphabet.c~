#include <stdio.h>

/**
 * main - A function that calls the print_alphabet
 * print_alphabet - A function that prints letters of the alphabet
 * Description: It prints the letter of the alphabets
 * Return: Always 0 (success)
 */

void print_alphabet(void)
{
	int ch;

	for (ch  = 'a'; ch <= 'z'; ch++)
	{
		putchar(ch);
	}
	putchar('\n');

}

int main(void)
{
	print_alphabet();

	return (0);

}
